# Sensitive Variables for Production Environment
# DO NOT COMMIT THIS FILE TO VERSION CONTROL
# This file should be created locally or injected via CI/CD

# PostgreSQL Admin Password
# Generate a strong password: openssl rand -base64 32
db_admin_password = "CHANGE-ME-STRONG-PASSWORD-HERE"

# Admin Initialization Secrets
# These are used by the /admin/api/init-admin-secure endpoint
# Generate strong secrets: openssl rand -base64 24
admin_init_secret = "CHANGE-ME-ADMIN-INIT-SECRET"
admin_init_password = "CHANGE-ME-ADMIN-PASSWORD"

# Optional: Override defaults
# admin_username = "admin"
# admin_email = "admin@sanbud.pl"
